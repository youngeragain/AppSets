<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="dockBackgroundLayerColor" format="reference"/>
    <attr name="chipBackgroundTintColor" format="reference"/>
    <attr name="screenBorderColor" format="color|reference"/>
    <attr name="userInfoBarBackgroundColor" format="color|reference"/>
    <declare-styleable name="RadiusCornerBlurView">
        <attr name="blurOverlayColor1" format="color"/>
        <attr name="cornerRadius1" format="float"/>
    </declare-styleable>

    <declare-styleable name="RipplePulseLayout">
        <attr name="rippleColor" format="color" />
        <attr name="rippleType" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
        </attr>
        <attr name="startRadius" format="dimension" />
        <attr name="endRadius" format="dimension" />
        <attr name="duration" format="integer" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AppSetsLogo">
        <attr name="line_one_color" format="color|reference|string"/>
        <attr name="line_two_color" format="color|reference|string"/>
        <attr name="line_three_color" format="color|reference|string"/>
        <attr name="line_four_color" format="color|reference|string"/>
        <attr name="show_coordinate" format="boolean"/>
        <attr name="line_thickness" format="float"/>
        <attr name="line_thickness_mode" format="enum">
            <enum name="factory" value="0"/>
            <enum name="custom" value="1"/>
        </attr>
        <attr name="background_color" format="float"/>
        <attr name="line_one_degree" format="float"/>
        <attr name="line_two_degree" format="float"/>
        <attr name="line_three_degree" format="float"/>
        <attr name="line_four_degree" format="float"/>
        <attr name="left_vertex_of_line_position" format="float"/>
    </declare-styleable>
    <declare-styleable name="NumberSwitchView">
        <attr name="ta_number_stroke_width" format="dimension" />
        <attr name="ta_number_bg_color" format="reference|color" />
        <attr name="ta_number_color" format="reference|color" />
        <attr name="ta_number_animation_duration" format="integer" />
    </declare-styleable>
</resources>